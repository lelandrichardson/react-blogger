{
  "name": "react-blogger",
  "version": "0.0.1",
  "description": "project description",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "start": "set NODE_ENV=production && npm run start-server",

    "build-client-dev": "node_modules/.bin/webpack --config webpack.dev.config.js",
    "build-client-prod": "node_modules/.bin/webpack --config webpack.prod.config.js",
    "build-server": "node_modules/.bin/webpack --config webpack.server.config.js",

    "watch-client": "node_modules/.bin/webpack --config webpack.config.js --watch",
    "watch-server": "node_modules/.bin/webpack --config webpack.server.config.js --watch",


    "start-server": "node build/server/server.js",
    "start-server-dev": "nodemon build/server/Server.js  --watch build/server",

    "create-migration": "node_modules/.bin/sequelize migration:create --config config.sequelize.json --models-path src/Models",
    "run-migration": "node_modules/.bin/sequelize db:migrate --config config.sequelize.json --models-path src/Models",
    "undo-migration": "node_modules/.bin/sequelize db:migrate:undo --config config.sequelize.json --models-path src/Models"
  },
